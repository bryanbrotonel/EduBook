<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="437.0" prefWidth="493.0"
	xmlns="http://javafx.com/javafx"
	xmlns:fx="http://javafx.com/fxml"
	fx:controller="application.RegisterController">
	<children>
		<TextField fx:id="regFirstNameTextField" layoutX="71.0"
			layoutY="113.0" promptText="First Name" />
		<TextField fx:id="regLastNameTextField" layoutX="263.0"
			layoutY="113.0" promptText="Last Name" />
		<TextField fx:id="regEmailTextField" layoutX="167.0"
			layoutY="167.0" promptText="Email" />
		<TextField fx:id="regPasswordTextField" layoutX="167.0"
			layoutY="212.0" promptText="Password" />
		<TextField fx:id="regConfirmPasswordTextField"
			layoutX="166.0" layoutY="264.0" promptText="Confirm Password" />
		<Button fx:id="registerUserBtn" layoutX="215.0" layoutY="328.0"
			mnemonicParsing="false" onAction="#registerBtnListener"
			text="Register" />
		<Label fx:id="logInLink" alignment="CENTER" layoutX="144.0"
			layoutY="379.0" onMouseClicked="#logInLinkListener" prefHeight="17.0"
			prefWidth="206.0" text="Already have an account? Log In"
			textAlignment="CENTER" />
		<Label alignment="CENTER" contentDisplay="CENTER"
			layoutX="144.0" layoutY="49.0" prefHeight="17.0" prefWidth="206.0"
			text="Register" textAlignment="CENTER">
			<font>
				<Font size="18.0" />
			</font>
		</Label>
		<Label fx:id="regErrorLabel" alignment="CENTER"
			contentDisplay="CENTER" layoutX="167.0" layoutY="82.0"
			prefHeight="17.0" prefWidth="161.0" textAlignment="CENTER"
			textFill="RED" />
	</children>
</AnchorPane>
