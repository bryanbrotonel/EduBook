<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.PasswordField?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.shape.Rectangle?>
<?import javafx.scene.text.Font?>

<AnchorPane prefHeight="560" prefWidth="850"
	style="-fx-background-color: WHITE;" stylesheets="@application.css"
	xmlns="http://javafx.com/javafx/15.0.1"
	xmlns:fx="http://javafx.com/fxml/1"
	fx:controller="application.RegisterController">
	<children>
		<Rectangle arcHeight="5.0" arcWidth="5.0" fill="#ffb600"
			height="560.0" stroke="TRANSPARENT" strokeType="INSIDE" width="425.0" />
		<Label layoutX="492.0" layoutY="85.0" prefHeight="29.0"
			prefWidth="289.0" text="Register" textFill="#434343">
			<font>
				<Font name="Raleway Bold" size="24.0" />
			</font>
		</Label>
		<TextField fx:id="regFirstNameTextField" layoutX="492.0"
			layoutY="134.0" prefHeight="27.0" prefWidth="289.0"
			promptText="First Name" />
		<TextField fx:id="regLastNameTextField" layoutX="492.0"
			layoutY="182.0" prefHeight="27.0" prefWidth="289.0"
			promptText="Last Name" />
		<TextField fx:id="regEmailTextField" layoutX="492.0"
			layoutY="227.0" prefHeight="27.0" prefWidth="289.0"
			promptText="Email" />
		<PasswordField fx:id="regPasswordTextField"
			layoutX="492.0" layoutY="270.0" prefHeight="27.0" prefWidth="289.0"
			promptText="Password" />
		<PasswordField fx:id="regConfirmPasswordTextField"
			layoutX="492.0" layoutY="317.0" prefHeight="27.0" prefWidth="289.0"
			promptText="Confirm Password" />
		<Button id="PrimaryButton" fx:id="registerUserBtn"
			layoutX="492.0" layoutY="382.0" mnemonicParsing="false"
			onAction="#registerBtnListener" prefHeight="27.0" prefWidth="289.0"
			text="Register" />
		<Label fx:id="logInLink" alignment="CENTER" layoutX="535.0"
			layoutY="431.0" onMouseClicked="#logInLinkListener" prefHeight="17.0"
			prefWidth="206.0" text="Already have an account? Log In"
			textAlignment="CENTER" textFill="#434343">
			<font>
				<Font name="Raleway Bold" size="13.0" />
			</font>
		</Label>
		<Label fx:id="regErrorLabel" alignment="CENTER"
			contentDisplay="CENTER" layoutX="557.0" layoutY="459.0"
			prefHeight="17.0" prefWidth="161.0" textAlignment="CENTER"
			textFill="RED">
			<font>
				<Font name="Raleway Regular" size="13.0" />
			</font>
		</Label>
		<Label id="PrimaryFont" layoutX="55.0" layoutY="230.0"
			prefHeight="79.0" prefWidth="128.0" styleClass="fuck" text="Edu"
			textFill="WHITE">
			<font>
				<Font name="Raleway Bold" size="64.0" />
			</font>
		</Label>
		<Label id="PrimaryFont" layoutX="183.0" layoutY="230.0"
			prefHeight="79.0" prefWidth="164.0" text="Book" textFill="#434343">
			<font>
				<Font name="Raleway Bold" size="64.0" />
			</font>
		</Label>
		<Label id="PrimaryFont" layoutX="55.0" layoutY="303.0"
			prefHeight="29.0" prefWidth="335.0"
			text="For Student and Professor Appointment Bookings"
			textFill="WHITE">
			<font>
				<Font name="Raleway Bold" size="14.0" />
			</font>
		</Label>
	</children>
</AnchorPane>
