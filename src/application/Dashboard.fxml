<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<fx:root maxHeight="-Infinity" maxWidth="-Infinity"
	minHeight="-Infinity" minWidth="-Infinity" prefHeight="560.0"
	prefWidth="650.0" type="BorderPane"
	xmlns="http://javafx.com/javafx"
	xmlns:fx="http://javafx.com/fxml">
	<top>
		<AnchorPane prefHeight="108.0" prefWidth="650.0"
			BorderPane.alignment="CENTER">
			<children>
				<Text fx:id="greetingText" layoutX="59.0" layoutY="76.0"
					strokeType="OUTSIDE" strokeWidth="0.0" text="Hello, User">
					<font>
						<Font name="System Bold" size="16.0" />
					</font>
				</Text>
				<Button fx:id="bookApptBtn" layoutX="461.0" layoutY="56.0"
					mnemonicParsing="false" text="Book Appointment"
					onAction="#bookApptBtnListener" />
			</children>
		</AnchorPane>
	</top>
	<center>
		<AnchorPane prefHeight="438.0" prefWidth="650.0"
			BorderPane.alignment="CENTER">
			<children>
				<TableView fx:id="apptTable" layoutX="104.0"
					layoutY="26.0" prefHeight="328.0" prefWidth="442.0">
					<columns>
						<TableColumn fx:id="apptCol" prefWidth="112.0"
							text="Appointment" />
						<TableColumn fx:id="profCol" prefWidth="98.0"
							text="Professor" />
						<TableColumn fx:id="dateCol" prefWidth="87.0"
							text="Date" />
						<TableColumn fx:id="startTimeCol" prefWidth="72.0"
							text="Start Time" />
						<TableColumn fx:id="endTimeCol" prefWidth="72.0"
							text="End Time" />
					</columns>
				</TableView>
			</children>
		</AnchorPane>
	</center>
	<bottom>
		<AnchorPane prefHeight="73.0" prefWidth="650.0"
			BorderPane.alignment="CENTER">
			<children>
				<Text fx:id="dashboardErrorText" fill="RED" layoutX="57.0"
					layoutY="28.0" strokeType="OUTSIDE" strokeWidth="0.0"
					wrappingWidth="282.21630859375" />
				<Button fx:id="editApptBtn" layoutX="454.0" layoutY="10.0"
					mnemonicParsing="false" onAction="#editApptBtnListener" text="Edit" />
				<Button fx:id="deleteApptBtn" layoutX="520.0" layoutY="10.0"
					mnemonicParsing="false" onAction="#deleteApptBtnListener"
					text="Delete" />
			</children>
		</AnchorPane>
	</bottom>
</fx:root>
