<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<fx:root maxHeight="-Infinity" maxWidth="-Infinity"
	minHeight="-Infinity" minWidth="-Infinity" prefHeight="560.0"
	prefWidth="650.0" type="BorderPane"
	xmlns="http://javafx.com/javafx/15.0.1"
	xmlns:fx="http://javafx.com/fxml/1">
	<top>
		<AnchorPane prefHeight="108.0" prefWidth="650.0"
			BorderPane.alignment="CENTER">
			<children>
				<Text fx:id="greetingText" fill="#434343" layoutX="50.0"
					layoutY="80.0" strokeType="OUTSIDE" strokeWidth="0.0"
					text="Hello, User">
					<font>
						<Font name="Raleway Bold" size="30.0" />
					</font>
				</Text>
				<Button fx:id="bookApptBtn" layoutX="464.0" layoutY="74.0"
					mnemonicParsing="false" onAction="#bookApptBtnListener"
					prefHeight="27.0" prefWidth="136.0" text="Book Appointment" />
				<Text fx:id="apptTodayText" fill="#949494" layoutX="50.0"
					layoutY="108.0" strokeType="OUTSIDE" strokeWidth="0.0"
					text="You have 0 appointments today"
					wrappingWidth="218.21630859375">
					<font>
						<Font name="System Bold" size="14.0" />
					</font>
				</Text>
			</children>
		</AnchorPane>
	</top>
	<center>
		<AnchorPane prefHeight="438.0" prefWidth="650.0"
			BorderPane.alignment="CENTER">
			<children>
				<TableView fx:id="apptTable" layoutX="53.0" layoutY="37.0"
					prefHeight="328.0" prefWidth="544.0">
					<columns>
						<TableColumn fx:id="apptCol" prefWidth="158.0"
							text="Appointment" />
						<TableColumn fx:id="profCol" prefWidth="111.0"
							text="Professor" />
						<TableColumn fx:id="dateCol" prefWidth="85.0"
							text="Date" />
						<TableColumn fx:id="startTimeCol" prefWidth="91.0"
							text="Start Time" />
						<TableColumn fx:id="endTimeCol" prefWidth="98.0"
							text="End Time" />
					</columns>
				</TableView>
			</children>
		</AnchorPane>
	</center>
	<bottom>
		<AnchorPane prefHeight="73.0" prefWidth="650.0"
			BorderPane.alignment="CENTER">
			<children>
				<Text fx:id="dashboardErrorText" fill="RED" layoutX="57.0"
					layoutY="28.0" strokeType="OUTSIDE" strokeWidth="0.0"
					wrappingWidth="282.21630859375" />
				<Button fx:id="editApptBtn" layoutX="455.0" layoutY="17.0"
					mnemonicParsing="false" onAction="#editApptBtnListener"
					prefHeight="27.0" prefWidth="56.0" text="Edit" />
				<Button fx:id="deleteApptBtn" layoutX="540.0" layoutY="17.0"
					mnemonicParsing="false" onAction="#deleteApptBtnListener"
					prefHeight="27.0" prefWidth="56.0" text="Delete" />
			</children>
		</AnchorPane>
	</bottom>
</fx:root>
