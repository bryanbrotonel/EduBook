<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.shape.Rectangle?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<BorderPane fx:id="mainPane" maxHeight="-Infinity"
	maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity"
	prefHeight="560.0" prefWidth="850.0"
	style="-fx-background-color: WHITE;"
	xmlns="http://javafx.com/javafx"
	xmlns:fx="http://javafx.com/fxml"
	fx:controller="application.ShellController">
	<left>
		<AnchorPane prefHeight="200.0" prefWidth="200.0"
			stylesheets="@application.css" BorderPane.alignment="CENTER">
			<children>
				<Rectangle arcHeight="5.0" arcWidth="5.0" fill="#ffb200"
					height="560.0" stroke="TRANSPARENT" strokeType="INSIDE"
					width="200.0" />
				<Text id="PrimaryFont" fx:id="dashboardNav" fill="#434343"
					layoutX="53.0" layoutY="179.0"
					onMouseClicked="#dashboardBtnListener" strokeType="OUTSIDE"
					strokeWidth="0.0" text="Dashboard" textAlignment="CENTER">
					<font>
						<Font name="Raleway Bold" size="18.0" />
					</font>
				</Text>
				<Text id="PrimaryFont" fx:id="settingsNav" fill="#434343"
					layoutX="63.0" layoutY="218.0"
					onMouseClicked="#settingsBtnListener" strokeType="OUTSIDE"
					strokeWidth="0.0" text="Settings" textAlignment="CENTER">
					<font>
						<Font name="Raleway Bold" size="18.0" />
					</font>
				</Text>
				<Label id="PrimaryFont" alignment="CENTER" layoutX="19.0"
					layoutY="50.0" prefHeight="44.0" prefWidth="69.0" text="Edu"
					textFill="WHITE">
					<font>
						<Font name="Raleway Bold" size="36.0" />
					</font>
				</Label>
				<Label id="PrimaryFont" alignment="CENTER" layoutX="88.0"
					layoutY="50.0" prefHeight="44.0" prefWidth="92.0" text="Book"
					textFill="#434343">
					<font>
						<Font name="Raleway Bold" size="36.0" />
					</font>
				</Label>
				<Button id="DangerButton" fx:id="logOutBtn" layoutX="48.0"
					layoutY="263.0" mnemonicParsing="false"
					onAction="#logOutBtnListener" prefHeight="35.0" prefWidth="92.0"
					text="Sign Out">
					<font>
						<Font size="16.0" />
					</font>
				</Button>
			</children>
		</AnchorPane>
	</left>
</BorderPane>
