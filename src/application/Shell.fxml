<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.shape.Rectangle?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<BorderPane fx:id="mainPane" maxHeight="-Infinity"
	maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity"
	prefHeight="560.0" prefWidth="850.0" xmlns="http://javafx.com/javafx"
	xmlns:fx="http://javafx.com/fxml"
	fx:controller="application.ShellController">
	<left>
		<AnchorPane prefHeight="200.0" prefWidth="200.0"
			BorderPane.alignment="CENTER">
			<children>
				<Rectangle arcHeight="5.0" arcWidth="5.0" fill="#ffb200"
					height="560.0" stroke="TRANSPARENT" strokeType="INSIDE"
					width="200.0" />
				<Text fx:id="dashboardNav" fill="#434343" layoutX="53.0"
					layoutY="179.0" onMouseClicked="#dashboardBtnListener"
					strokeType="OUTSIDE" strokeWidth="0.0" text="Dashboard">
					<font>
						<Font name="Raleway Bold" size="18.0" />
					</font>
				</Text>
				<Text fx:id="settingsNav" fill="#434343" layoutX="64.0"
					layoutY="216.0" onMouseClicked="#settingsBtnListener"
					strokeType="OUTSIDE" strokeWidth="0.0" text="Settings">
					<font>
						<Font name="Raleway Bold" size="18.0" />
					</font>
				</Text>
				<Text fx:id="logOutBtn" fill="#434343" layoutX="65.0"
					layoutY="275.0" onMouseClicked="#logOutBtnListener"
					strokeType="OUTSIDE" strokeWidth="0.0" text="Log Out">
					<font>
						<Font name="Raleway Bold" size="18.0" />
					</font>
				</Text>
				<Label alignment="CENTER" layoutX="19.0" layoutY="50.0"
					prefHeight="44.0" prefWidth="69.0" text="Edu" textFill="WHITE">
					<font>
						<Font name="Raleway Bold" size="36.0" />
					</font>
				</Label>
				<Label alignment="CENTER" layoutX="88.0" layoutY="50.0"
					prefHeight="44.0" prefWidth="92.0" text="Book" textFill="#434343">
					<font>
						<Font name="Raleway Bold" size="36.0" />
					</font>
				</Label>
			</children>
		</AnchorPane>
	</left>
</BorderPane>
