<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>
<?import javafx.scene.text.Text?>

<AnchorPane prefHeight="400.0" prefWidth="422.0" xmlns="http://javafx.com/javafx/15.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="application.LoginController">
   <children>
      <Label alignment="CENTER" layoutX="109.0" layoutY="59.0" prefHeight="17.0" prefWidth="206.0" text="EduBook" textAlignment="CENTER">
         <font>
            <Font size="18.0" />
         </font>
      </Label>
      <TextField fx:id="emailTextField" layoutX="129.0" layoutY="122.0" promptText="Email" />
      <TextField fx:id="passwordTextField" layoutX="128.0" layoutY="158.0" promptText="Password" />
      <Button fx:id="selectBtn" layoutX="131.0" layoutY="206.0" mnemonicParsing="false" onAction="#loginBtnListener" prefHeight="27.0" prefWidth="161.0" text="Log In" />
      <Text fx:id="registerBtn" layoutX="124.0" layoutY="274.0" onMouseClicked="#registerBtnListener" strokeType="OUTSIDE" strokeWidth="0.0" text="Don't have an account? Register" textAlignment="CENTER" />
   </children>
</AnchorPane>
